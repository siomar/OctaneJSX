{
  "compileOnSave": true,
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "removeComments": true,
    "preserveConstEnums": true,
    "strictNullChecks": false,
    "jsx": "react",
    "jsxFactory": "Octane.createElement",
    "module": "es2015",
    "target": "es6"
  },
  "include": ["./src/**/*.tsx"]
}
